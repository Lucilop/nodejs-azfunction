@Library('jenkins-library@feature/cycletimemethods')_

pipeline {
    agent any
    stages {
        stage('Prepare') {
            steps {
                script {
                    echo 'Prepare variables , functions and so on'
                }
            }
        }
    }

        post {
            always {
                sendCycleTimeData( environment:'integration' , jiraissukey: env.JIRA_ISSUE_KEY )
            }
            cleanup {
                cleanWs()
            }
        }
}

