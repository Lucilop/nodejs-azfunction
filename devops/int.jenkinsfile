@Library('jenkins-library@feature/cycletimemethods')_

pipeline {
    agent any
    stages {
        stage('Prepare') {
            steps {
                script {
                    echo 'Prepare variables , functions and so on'
                }
            }
        }
    }

        post {
            success {
                sendCycleTimeDataInt(jiraIssueKey: env.JIRA_ISSUE_KEY )
            }
            cleanup {
                cleanWs()
            }
        }
}

